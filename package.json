{
  "name": "Decky-Plugin-Template",
  "version": "0.0.1",
  "description": "Decky plugin template",
  "scripts": {
    "build": "clear && echo \"Building plugin\" && shx rm -rf dist && rollup -c",
    "watch": "clear && echo \"Watching for changes with rollup\" && rollup -c -w",
    "test": "clear && echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Emiliopg91/decky-plugin-template.git"
  },
  "keywords": [
    "decky",
    "plugin",
    "template"
  ],
  "author": "Emiliopg91",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Emiliopg91/decky-plugin-template/issues"
  },
  "homepage": "https://github.com/Emiliopg91/decky-plugin-template#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.5",
    "decky-frontend-lib": "^3.25.0",
    "decky-plugin-framework": "^1.2.1",
    "rollup": "^2.79.1",
    "rollup-plugin-import-assets": "^1.1.1",
    "shx": "^0.3.4",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "react-icons": "^4.12.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "decky-frontend-lib"
      ]
    }
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}